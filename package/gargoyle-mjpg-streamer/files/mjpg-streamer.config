
config mjpg-streamer 'core'
	option enabled '0'
	option input 'uvc'
	option output 'http'
	option device '/dev/video0'
	option resolution '640x480'
	option yuv '0'
	option quality '80'
	option fps '5'
	option led 'auto'
	option www '/www/webcam'
	option port '8080'
	option username 'admin'
	option password 'admin'
